                                  <?php
                                    $q4 = "select coursecat, classid from courseclass ";
                                    $r4 = mysql_query($q4);
                                    while (list($courseclass, $classid) = mysql_fetch_array($r4)) {
                                  ?>

                                      <option value="<?php echo "$classid"; ?>" name="<?php echo "$classid"; ?>"><?php echo "$courseclass"; ?> </option>
                                  <?php
                                    }
                                   ?>

                                </select>
                            </div>
                        </div><!--/.col-lg-3-->

